          <div class="header">Vorteile</div>
          <div class="box">
            <div class="box-inner">
              {% for advantage in character.advantages %}
                <div class="case">{{ advantage.name }}</div>
              {% endfor %}
            </div>
          </div>
          <div class="header">Nachteile</div>
          <div class="box">
            <div class="box-inner">
              {% for disadvantage in character.disadvantages %}
                <div class="case">{{ disadvantage.name }}</div>
              {% endfor %}
