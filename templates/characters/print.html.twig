            <div class="box">
              <div class="box-inner">
                {{ character.experiencelevel }}
              </div>
            </div>
            <div class="boxdescription">Erfahrungsgrad</div>
          </div>
          <div class="row">
            <div class="box">
              <div class="box-inner">
                {{ character.aptotal }}
              </div>
            </div>
            <div class="boxdescription">AP gesamt</div>
          </div>
          <div class="row">
            <div class="box">
              <div class="box-inner">
                {{ character.apavailable }}
              </div>
            </div>
            <div class="boxdescription">Ap verfügbar</div>
          </div>
          <div class="row">
            <div class="box">
              <div class="box-inner">
                {{ character.apspent }}
              </div>
            </div>
            <div class="boxdescription">AP ausgegeben</div>
          </div>
        </div>
