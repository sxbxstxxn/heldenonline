                <div class="value-inner">{{character.speed}}</div>
              </div>
            </div>
            <div class="boxvalue">
              <div class="value">
                <div class="value-inner">{{character.speedbonus}}</div>
              </div>
            </div>
            <div class="boxvalue">
              <div class="value">
                <div class="value-inner">X</div>
              </div>
            </div>
            <div class="boxvalue">
              <div class="value">
                <div class="value-inner">{{character.speedmax}}</div>
              </div>
            </div>
          </div>
          <div class="row fatepoints">
            <div class="boxheader">
              <div class="header">Schicksalspunkte</div>
            </div>
            <div class="boxvalue">
              <div class="valuedescription">Wert</div>
              <div class="value">
                <div class="value-inner">{{ character.fatepoints }}</div>
              </div>
            </div>
            <div class="boxvalue">
              <div class="valuedescription">Bonus</div>
              <div class="value">
                <div class="value-inner">{{ character.fatepointsbonus }}</div>
              </div>
            </div>
            <div class="boxvalue">
              <div class="valuedescription">Max</div>
              <div class="value">
                <div class="value-inner">{{ character.fatepointsmax }}</div>
              </div>
            </div>
            <div class="boxvalue boxvalue-big">
              <div class="valuedescription">Aktuell</div>
              <div class="value">
                <div class="value-inner">{{ character.fatepointscurrent }}</div>
              </div>
            </div>
          </div>
          <div class="row annotation">
            <div class="boxheader">
              <div class="header">Anmerkungen</div>
            </div>
            <div class="boxvalue">
              <div class="boxvalue-inner">
                {{ character.annotation}}
              </div>
            </div>
          </div>
        </div>
      </div>
