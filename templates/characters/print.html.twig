<html>
<head>
  <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('images/favicons/apple-touch-icon.png') }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('images/favicons/favicon-32x32.png') }}">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('images/favicons/favicon-16x16.png') }}">
  <link rel="manifest" href="{{ asset('images/favicons/site.webmanifest') }}">
  <link rel="mask-icon" href="{{ asset('images/favicons/safari-pinned-tab.svg') }}" color="#343a40">
  <link rel="shortcut icon" href="{{ asset('images/favicons/favicon.ico') }}">
  <title>{% block title %}Helden Online - {{ character.charname }} - Druckansicht{% endblock %}</title>
  {% block stylesheets %}
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Play&display=swap" rel="stylesheet">
    <!-- Helden Online CSS -->
    <link href="{{ asset('css/print.css') }}" rel="stylesheet">
  {% endblock %}
</head>
<body>
  <div class="character">
    <div class="character-page personal-data">
      <div class="character-page-printview">
        <div class="logo">
          <img class="ulisseslogo" src="{{ asset('images/logo-fanseite.png') }}"/>
          <div class="heldenonline">https://helden.online</div>
          <div class="document">Persönliche Daten</div>
        </div>
        <div class="attributes">
          <div class="box">
            <div class="box-inner">
              <div class="header">MU</div>
              <div class="value">{{character.attributeMu}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">KL</div>
              <div class="value">{{character.attributeKl}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">IN</div>
              <div class="value">{{character.attributeIn}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">CH</div>
              <div class="value">{{character.attributeCh}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">FF</div>
              <div class="value">{{character.attributeFf}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">GE</div>
              <div class="value">{{character.attributeGe}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">KO</div>
              <div class="value">{{character.attributeKo}}</div>
            </div>
          </div>
          <div class="box">
            <div class="box-inner">
              <div class="header">KK</div>
              <div class="value">{{character.attributeKk}}</div>
            </div>
          </div>
          <div class="name">
            <div class="name-inner">{{ character.charname }}</div>
          </div>
        </div>
        <div class="story">
          <div class="row">
            <div class="header">Familie</div>
            <div class="value">{{ character.family }}</div>
            <div class="header">Geburtsort</div>
            <div class="value">{{ character.birthplace }}</div>
          </div>
          <div class="row">
            <div class="header">Geburtsdatum</div>
            <div class="value">{{ character.birthdate }}</div>
            <div class="header-small">Alter</div>
            <div class="value-small">{{ character.age }}</div>
            <div class="header-small">Geschlecht</div>
            <div class="value-small">{{ character.gender }}</div>
          </div>
          <div class="row">
            <div class="header">Spezies</div>
            <div class="value">{{ character.species.name }}</div>
            <div class="header-small">Größe</div>
            <div class="value-small">{{ character.size }}</div>
            <div class="header-small">Gewicht</div>
            <div class="value-small">{{ character.weight }}</div>
          </div>
          <div class="row">
            <div class="header">Haarfarbe</div>
            <div class="value">{{ character.haircolor }}</div>
            <div class="header">Augenfarbe</div>
            <div class="value">{{ character.eyecolor }}</div>
          </div>
          <div class="row">
            <div class="header">Kultur</div>
            <div class="value">{{ character.culture.name }}</div>
            <div class="header">Profession</div>
            <div class="value">{{ character.profession.name }}</div>
          </div>
          <div class="row">
            <div class="header">Titel</div>
            <div class="value">{{ character.title }}</div>
            <div class="header">Sozialstatus</div>
            <div class="value">{{ character.socialstatus }}</div>
          </div>
          <div class="row">
            <div class="header">Charakteristika</div>
            <div class="value-big">{{ character.characteristics }}</div>
          </div>
          <div class="row">
            <div class="header">Sonstiges</div>
            <div class="value-big">{{ character.further }}</div>
          </div>
        </div>
        <div class="experience">
