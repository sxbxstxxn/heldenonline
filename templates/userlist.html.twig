<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })
</script>
{% for user in userlist %}
    {% if date().timestamp - user.lastActivityAt < 600 %}
        <span class="d-inline-block" tabindex="0" data-toggle="tooltip" data-placement="right" title="Letzte AktivitÃ¤t vor {{ (date().timestamp - user.lastActivityAt) // 60 }} Minuten">
            <button type="button" class="btn btn-secondary btn-sm" style="pointer-events: none;" disabled>{{ user.username }}</button>
        </span>
    {% endif %}
{% endfor %}